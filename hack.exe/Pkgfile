name=hack.exe
version=git
release=1
source=(https://raw.githubusercontent.com/janbrennen/rice/master/hack.exe.c)

build() {
	gcc -pthread hack.exe.c -o hack.exe
	install -Dm755 hack.exe $PKG/usr/bin/hack.exe
}
